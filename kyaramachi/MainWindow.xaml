<Window x:Class="kyaramachi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kyaramachi"
        mc:Ignorable="d"
        Title="MainWindow" SizeToContent="WidthAndHeight">
    <Grid Name="win" Width="256" Height="224">
        <Image x:Name="imgBg" Stretch="None" Margin="120 112 0 0" />
        <Image x:Name="imgChara" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="None" />
    </Grid>
</Window>
